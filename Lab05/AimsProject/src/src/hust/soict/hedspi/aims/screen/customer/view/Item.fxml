<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane style="-fx-border-color: black;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" prefHeight="175.0" prefWidth="305.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="lbTitle" alignment="CENTER" prefHeight="50.0" prefWidth="305.0" text="Title" />
            <Label fx:id="lbCost" alignment="CENTER" prefHeight="50.0" prefWidth="305.0" text="Cost" />
            <HBox alignment="CENTER" prefHeight="50.0" prefWidth="305.0" spacing="20.0">
               <children>
                  <Button fx:id="btnAddToCart" onAction="#btnAddToCartClicked" text="Add to Cart" />
                  <Button fx:id="btnPlay" onAction="#btnPlayClicked" text="Play" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
